{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "declaration": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./*"],
    },
    "types": [
      "web-bluetooth"
    ]
  },
  "include": [
    "./src/**/*"
  ],

}
